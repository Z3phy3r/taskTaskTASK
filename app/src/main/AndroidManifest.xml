<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.builders.taskmaster">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.taksmasterapp.Friends" />
        <activity android:name="com.example.taksmasterapp.submissions"/>
        <activity android:name="com.example.taksmasterapp.TmTask"/>
        <activity android:name="com.example.taksmasterapp.FirstTimeSetup"/>
        <activity android:name="com.example.taksmasterapp.LogIn"/>
        <activity android:name="com.example.taksmasterapp.Profile"/>
        <activity android:name="com.example.taksmasterapp.Settings"/>
        <activity android:name="com.example.taksmasterapp.UploadTask"/>
    </application>

</manifest>